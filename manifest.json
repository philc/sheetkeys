{
  "manifest_version": 2,
  "name": "Sheetkeys",
  "version": "1.0.4",
  "description": "TODO",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/spreadsheets/*",
                  "https://docs.google.com/a*/spreadsheets/*"],
      "js": ["content_scripts/keyboard_utils.js",
